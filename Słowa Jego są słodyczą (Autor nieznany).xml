<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.2" modifiedIn="OpenLP 2.9.2" modifiedDate="2018-05-10T11:59:54">
  <properties>
    <titles>
      <title>Słowa Jego są słodyczą</title>
    </titles>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="o1">
      <lines>Słowa Jego są słodyczą<br/>i cały pełen jest powabu,<br/>Taki jest miły mój,<br/>oto przyjaciel mój,<br/>córki Jeruzalem.</lines>
    </verse>
  </lyrics>
</song>
