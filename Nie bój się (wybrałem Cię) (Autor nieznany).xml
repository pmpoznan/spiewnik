      <title>Nie bój się (wybrałem Cię)</title>
    </titles>
    <authors>
      <author type="words">Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Nie bój się<br/>Nie lękaj się<br/>Wybrałem Cię<br/>Wezwałem Cię po imieniu<br/>Tyś moja jest</lines>
    </verse>
  </lyrics>
