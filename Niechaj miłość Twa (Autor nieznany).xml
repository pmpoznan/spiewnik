<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.2" modifiedIn="OpenLP 2.9.2" modifiedDate="2021-09-17T21:08:41">
  <properties>
    <titles>
      <title>Niechaj miłość Twa</title>
    </titles>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="o1">
      <lines>Niechaj miłość Twa<br/>jak potężna fala<br/>spłynie tu, przez łaski Twej zdrój<br/>Chryste dotknij mnie</lines>
    </verse>
  </lyrics>
</song>
