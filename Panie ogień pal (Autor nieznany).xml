  <properties>
    <titles>
      <title>Panie ogień pal</title>
    </titles>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="o1">
      <lines>Panie, ogień pal, wypal smutek i żal<br/>Wypal myśli natrętne, niechciane,<br/>wypal to, co winno być zapomniane</lines>
    </verse>
  </lyrics>
