<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.2" modifiedIn="OpenLP 2.9.2" modifiedDate="2021-09-17T21:08:42">
  <properties>
    <titles>
      <title>Panie proszę spraw</title>
    </titles>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="o1">
      <lines>Panie proszę spraw<br/>Aby życie mi nie było obojętne<br/>Abym zawsze kochał, to co piękne<br/>Pozostawił ciepły ślad na czyjejś ręce</lines>
    </verse>
  </lyrics>
</song>
